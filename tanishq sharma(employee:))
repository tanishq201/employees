#include<stdio.h>
#include<conio.h>
struct employee
{
int empno;
int employee name;
int age;
int number;
int salary;
};
void display()
{
struct employee s[20];
int i;
for(i=0;i<20;i++)
{
s[i].empno=i+1;
printf("\nEmpno=%d",s[i].empno);
printf("\nEnter employee name:");
scanf("%s",s[i].employee name);
printf("\nEnter age:");
scanf("%d",&s[i].age);
printf("\nEnter phone number:");
scanf("%d",&s[i].number);
printf("\nEnter salary:");
scanf("%d",&s[i].salary);
}
for(i=0;i<20;i++)
{
printf("\nEmp no.:%d",s[i].empno);
printf("\nEmployee name:%d",s[i].employee name);
printf("\nEnter age:%d",s[i].age);
printf("\nEnter phone number:%d",s[i].number);
printf("\nEnter salary:%d",s[i].salary);
}
}
int main()
{
printf("\n enter the details");
display();
printf("\n displayed successfully");
}
